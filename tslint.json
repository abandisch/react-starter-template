{
  "extends": ["tslint-react", "tslint-config-prettier", "tslint-config-standard"],
  "rules": {
    // override tslint-* rules here
    "jsx-boolean-value": false
  },
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/registerServiceWorker.ts"
    ]
  }
}
